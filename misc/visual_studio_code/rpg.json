{
  "MUTE equal": {
    "prefix": "mue",
    "body": [
      "MU* VAL1($1) VAL2($2) COMP(EQ)"
    ],
    "description": "MUTE annotation for equality"
  },
  "MUTE fail": {
    "prefix": "muf",
    "body": [
      "MU* FAIL($1)"
    ],
    "description": "MUTE fail assertion"
  },
  "CALL": {
    "prefix": "call",
    "body": [
      "C                   call      $1"
    ],
    "description": "CALL stmt"
  },
  "CALL parm": {
    "prefix": "parm",
    "body": [
      "C                   parm                    $1"
    ],
    "description": "Call parameter"
  },
  "SETON LR": {
    "prefix": "lr",
    "body": [
      "C                   seton                                        lr"
    ],
    "description": "Seton LR"
  },
  "EVAL": {
    "prefix": "ev",
    "body": [
      "C                   EVAL      $1=$2"
    ],
    "description": "Eval"
  },
  "DN": {
    "prefix": "dn",
    "body": [
      "D x               S             00  0 inz(00)"
    ],
    "description": "Declare Numeric variable"
  },
  "IF": {
    "prefix": "if",
    "body": [
      "C                   IF        $1=$2"
    ],
    "description": "If statement"
  },
  "DSPLY": {
    "prefix": "dsp",
    "body": [
      "C                   DSPLY                   $1"
    ],
    "description": "DSPLY statement"
  },
  "ENTRY": {
    "prefix": "ent",
    "body": [
      "C     *entry        plist"
    ],
    "description": "Entry plist"
  }
}